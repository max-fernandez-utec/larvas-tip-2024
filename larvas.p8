function _init()
	debug=true
	ents={}
	parts={}
	balas={}

	escenario={
		x0=-200,y0=0,
		xy=200,y1=400,
	}
		
	cam=make_cam()
	
	-- setup jugador
	jug=make_larva()
	jug.x=5
	jug.y=0
	jug.fh=true
end

function _update()
	for e in all(ents) do
		e.upd()
	end
end

function _draw()
	cls(12)
	
	-- camera(cam.x,cam.y)
	
	-- fondo
	--rectfill(-100,116,250,127,3)
	map()
	drw_agua()
	
	-- parte del frente
	for e in all(ents) do
		e.drw()
	end
	
	print(jug.dy,7)
		--cam.x+1,cam.y+1,7)
end


