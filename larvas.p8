-- ambiente
agua={
	f=0, fr={80,80,80,80,80,81,81,81,81}
}

function drw_agua()
	agua.f+=1
	
	if (agua.f>#agua.fr) agua.f=1
	
	for i=0,15 do
		local offset=i%#agua.fr
		local frame=agua.f+offset
		
		if (frame>#agua.fr) frame=1
		
		spr(agua.fr[frame],8*i,120)
	end
end
