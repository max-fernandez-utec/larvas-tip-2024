-- entidades
function make_entity()
	local e={}
	e.x=0
	e.y=0
	e.s=0
	e.dx=0
	e.dy=0
	e.fr=nil
	e.stat=nil
	
	add(ents,e)
	
	return e
end

function make_larva()
	local e=make_entity()
	e.s=1
	e.fh=true
	e.fr={
		walk={ 1,5 },
		idle={ 1,3 }
	}
	local grv=0.1
	
	local idle="idle"
	local walk="walk"
	e.stat=idle
	
	e.a=0
	e.p=0
	e.d=false
	e.cx=5+(e.fh and 5 or 0)
	e.cy=11
	
	local va=0.01
	local dp=0.01
	local vlarva=1
	
	e.upd=function()
		e.cx=5+(e.fh and 5 or 0)
		
		e.stat=idle
		e.dx=0
		if btn(âž¡ï¸) then
			e.fh=true
			e.stat=walk
			
			e.dx=vlarva
		end
		
		if btn(â¬…ï¸) then
			e.fh=false
			e.stat=walk
			
			e.dx=-vlarva
		end
		
		if (btn(â¬†ï¸)) jug.a=min(jug.a+va,1/4)
		if (btn(â¬‡ï¸)) jug.a=max(jug.a-va,-1/4)
		if (btnp(ðŸ…¾ï¸)) e.dy=-2
		
		if #balas==0 then
			if btn(âŽ) then
				e.d=true
				e.p+=dp
				e.p=min(1,e.p)
			end
		end
			
		if e.d and (not btn(âŽ) or e.p==1) then
			make_disparo(e.x+e.cx,e.y+e.cy,e.p,e.a)
			e.p=0
			e.d=false
		end
		
		e.x+=e.dx
		
		-- gravedad
		e.dy+=grv
		
		abajo=e.y+16
	 tilex=e.x\8
	 tiley=abajo\8
		
		if e.dy>0 and fget(mget(tilex,tiley),0) then
			e.dy=0
			e.y=(tiley-2)*8
		end
		
		e.y+=e.dy
	end
	
	e.drw=function()
		local sps=e.fr[e.stat]
		e.s+=.25
		
		if flr(e.s)>#sps then
			e.s=1
		end
		-- dibujar cuerpo
		palt(0,false)
		palt(11,true)
		
		spr(sps[flr(e.s)],
			e.x+(e.fh and 8 or 0),
			e.y,1,2,
			e.fh)
	
		-- dibujar cola
		spr(sps[flr(e.s)]+17,
			e.x+(e.fh and 0 or 8),
			e.y+8,
			1,1,
			e.fh)
			
		palt()
		
		-- calcular centro de mira
		-- centro en (5,11)
		local md=24
		local mlado=e.fh and 1 or -1
		local mvx=mlado*(md*cos(e.a))
		local mvy=md*sin(e.a)
		
		-- dibujar poder
		local lvx=e.x+e.cx+mvx*e.p
		local lvy=e.y+e.cy+mvy*e.p
		line(e.x+e.cx,e.y+e.cy,lvx,lvy,7)
		--circfill(e.x+e.cx+lvx*.25,e.y+e.cy+lvy*.25,2.5*e.p,10)
		--circfill(e.x+e.cx+lvx*.5,e.y+e.cy+lvy*.5,3*e.p,10)
		--circfill(e.x+e.cx+lvx*.75,e.y+e.cy+lvy*.75,3.5*e.p,10)
		circfill(lvx,lvy,4*e.p,10)
		
		-- dibujar mira
		spr(2,e.x+e.cx+mvx-3,e.y+e.cy+mvy-3)
	end
	
	return e
end

function make_disparo (x,y,pod,ang)
	local e=make_entity()
	local vel=4
	local grv=0.06
	
	add(balas,e)
	
	e.x=x
	e.y=y
	e.dx=vel*pod*cos(ang)
	e.dy=vel*pod*sin(ang)
	
	e.upd=function()
		e.dy+=grv
	
		e.x+=e.dx
		e.y+=e.dy
		
		if (e.y>200) then
			del(balas,e)
			del(ents,e)
		end
	end
	
	e.drw=function()
		circfill(e.x,e.y,3,10)
		circfill(e.x,e.y,2,9)
	end
end

function make_cam()
	local e=make_entity()
	
	e.upd=function()
		if #balas>0 then
			cam.x=balas[1].x-63
			cam.y=balas[1].y-63
		else
			cam.x=jug.x
			cam.y=jug.y-100
		end
	end
	
	e.drw=function()
	end
	
	return e
end
